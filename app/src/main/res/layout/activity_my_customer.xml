<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:layout_width="match_parent"
              android:layout_height="match_parent"
              android:background="@color/bg_f0"
              android:orientation="vertical">

    <include layout="@layout/layout_head"/>

    <com.zemult.yovollserver.view.SearchView
        android:id="@+id/search_view"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>

    <include layout="@layout/layout_head_my_customer"/>

    <com.zemult.yovollserver.view.SmoothListView.SmoothListView
        android:id="@+id/smooth_list_view"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:divider="@null"
        android:dividerHeight="0dp"
        android:fadingEdge="none"
        android:overScrollMode="never"
        android:scrollbars="none">
    </com.zemult.yovollserver.view.SmoothListView.SmoothListView>

    <include layout="@layout/item_no_search_data_layout"/>


</LinearLayout>